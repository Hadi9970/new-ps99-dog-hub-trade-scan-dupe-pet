-- Booting Library
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
 
 
-- Functions
_G.Key = "hdg1D_5shHs_jsT4g"
_G.KeyInput = "string"
 
function DestroyGui()
    OrionLib:Destroy()
end
 
function Menu()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Hadi9970/New-ps99-dog-hub-trade-scam-hub-dupe/main/dog%20hub"))()
end"))()
end
 
 
-- Creating Window
local Window = OrionLib:MakeWindow({Name = "dog hun key system", HidePremium = false, IntroEnabled = false})
 
 
-- Creating Tabs
local tab1 = Window:MakeTab({
	Name = "key",
	Icon = "",
	PremiumOnly = false
})
 
local tab2 = Window:MakeTab({
	Name = "delete gui",
	Icon = "",
	PremiumOnly = false
})
 
 
-- Config Tab1
local Section1 = tab1:AddSection({
	Name = "Key"
})
 
tab1:AddTextbox({
	Name = "enter key",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
         _G.KeyInput = Value
	end	  
})
 
tab1:AddButton({
	Name = "check key",
	Callback = function()
        if _G.KeyInput == _G.Key then
            OrionLib:MakeNotification({
	        Name = "Correct key!",
	        Content = "Correct key script is loded",
	        Image = "",
	        Time = 1.5
            })
        wait(1.5)
        Menu()
    end
	end    
})
 
-- Config Tab2
local Section1 = tab2:AddSection({
	Name = "DestroydScript"
})
 
 
 
 
 
local Section1 = tab2:AddSection({
	Name = "Delete the script"
})
 
tab2:AddButton({
	Name = "",
	Callback = function()
	    wait(0.5)
	    DestroyGui()
	end    
})
-- Script End
OrionLib:Init()
